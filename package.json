{
  "name": "kanban-board",
  "version": "1.0.0",
  "description": "A beautiful and functional Kanban board application built with Next.js, React, and TypeScript",
  "author": "Kanban Board Team",
  "license": "MIT",
  "keywords": [
    "kanban",
    "task-management",
    "productivity",
    "react",
    "nextjs",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/kanban-board.git"
  },
  "scripts": {
